{
  "version": 2,
  "framework": null,
  "installCommand": "cd frontend && npm install --no-audit --no-fund",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
